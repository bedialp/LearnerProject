﻿@model LearnerProject.Models.Entities.Banner
@{
    ViewBag.Title = "UpdateBanner";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h3>Banner Guncelle</h3>

<form method="post">

    @Html.HiddenFor(x => x.BannerId)
    <label>Baslik</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary"><i class="fa fa-pen"></i> Güncelle</button>
</form>